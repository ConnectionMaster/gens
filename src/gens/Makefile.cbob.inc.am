# Makefile.am for binary files.

CBOB = $(top_builddir)/src/cbob/cbob$(EXEEXT)

# TODO: Write macros to make this easier to maintain.
bin/32X/bios32xg.bin.c:
	$(CBOB) $(srcdir)/bin/32X/bios32xg.bin $@
bin/32X/bios32xm.bin.c:
	$(CBOB) $(srcdir)/bin/32X/bios32xm.bin $@
bin/32X/bios32xs.bin.c:
	$(CBOB) $(srcdir)/bin/32X/bios32xs.bin $@

gens_SOURCES += \
		bin/32X/bios32xg.bin.c \
		bin/32X/bios32xm.bin.c \
		bin/32X/bios32xs.bin.c
